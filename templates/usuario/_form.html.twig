<div class="x_content">
	<div class="col-md-123 center-margin">
		{{ form_start(form, {'attr':{'class': 'formValidate form-horizontal form-label-left'} }) }}
		<div class="form-group row">
			<label>Apellido paterno</label>
			{{ form_widget(form.apellidoPaterno) }}
		</div>
		<div class="form-group row">
			<label>Apellido Materno</label>
			{{ form_widget(form.apellidoMaterno) }}
		</div>
		<div class="form-group row">
			<label>Nombres</label>
			{{ form_widget(form.username) }}
		</div>
		<div class="form-group row">
			<label>Número de identificación</label>
			{{ form_widget(form.numeroIdentificacion) }}
		</div>

		<div class="form-group row">
			<label>País</label>
			{{ form_widget(form.pais) }}
		</div>
		<div class="form-group row">
			<label>Especialidad</label>
			{{ form_widget(form.especialidad) }}
		</div>
		<div class="form-group row">
			<label>Estado</label>
			{{ form_widget(form.estado) }}
		</div>

		<div class="ln_solid"></div>
		<h3>Datos de usuario</h3>
		<div class="form-group row">
			<label>Email</label>
			{{ form_widget(form.email) }}
		</div>
		<div class="form-group row">
			<label>Tipo</label>
			{{ form_widget(form.role_option) }}
		</div>
		<div class="ln_solid"></div>
		<div class="item form-group">
			<div class="">
				<button type="submit" class="btn btn-info btn-lg">
					<i class="fa fa-save"></i>
					Guardar</button>
				<a href="{{path('usuario_index')}}" class="btn btn-warning">
					<i class="fa fa-ban"></i>
					Cancelar</a>

			</div>
		</div>
		{{ form_widget(form._token) }}
		{{ form_end(form,{'render_rest':false}) }}
	</div>
</div>
